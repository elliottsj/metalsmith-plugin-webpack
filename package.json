{
  "name": "metalsmith-webpack",
  "version": "1.0.1",
  "description": "A webpack plugin for Metalsmith",
  "keywords": [
    "metalsmith",
    "javascript",
    "webpack"
  ],
  "author": "Christopher Cliff <ctcliff@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/christophercliff/metalsmith-webpack"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel lib --out-dir dist",
    "mocha": "mocha -r should --reporter spec",
    "test": "standard && npm run build && npm run mocha"
  },
  "engines": {
    "node": ">=0.10",
    "npm": "^2.0.0"
  },
  "dependencies": {
    "memory-fs": "^0.2.0",
    "webpack": "^1.10.0"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.0.1",
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.20",
    "metalsmith": "^1.7.0",
    "mocha": "^2.2.5",
    "should": "^7.0.1",
    "standard": "^4.4.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules",
      "/dist/",
      "*bundle.js"
    ]
  }
}
