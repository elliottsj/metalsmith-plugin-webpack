{
  "version": "1.1.0",
  "name": "metalsmith-plugin-webpack",
  "description": "A webpack plugin for Metalsmith",
  "keywords": [
    "metalsmith",
    "javascript",
    "webpack"
  ],
  "contributors": [
    "Christopher Cliff <ctcliff@gmail.com>",
    "Spencer Elliott <me@elliottsj.com>"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/christophercliff/metalsmith-webpack"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel lib --out-dir dist",
    "mocha": "mocha -r should --reporter spec",
    "test": "standard && npm run build && npm run mocha"
  },
  "engines": {
    "node": ">=0.10",
    "npm": "^2.0.0"
  },
  "dependencies": {
    "memory-fs": "^0.4.1",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.0.1",
    "babel": "^6.23.0",
    "babel-eslint": "^7.2.3",
    "metalsmith": "^2.3.0",
    "mocha": "^3.4.2",
    "should": "^11.2.1",
    "standard": "^10.0.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules",
      "/dist/",
      "*bundle.js"
    ]
  }
